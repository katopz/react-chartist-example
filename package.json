{
  "name": "react-chartist-example",
  "version": "1.0.0",
  "description": "Working example for react chartist.",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "test": "npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "eslint {src,test}"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-minimal": "^2.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "cross-env": "^2.0.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.14.0",
    "json-loader": "^0.5.4",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.11.0",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "dependencies": {
    "preact": "^5.7.0",
    "preact-compat": "^3.0.0",
    "proptypes": "^0.14.3",
    "react": "^15.3.1",
    "react-chartist": "^0.10.2",
    "react-dom": "^15.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katopz/react-chartist-example.git"
  },
  "keywords": [
    "react",
    "chartist",
    "example"
  ],
  "author": "katopz <katopz@gmail.com> (https://katopz.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katopz/react-chartist-example/issues"
  },
  "homepage": "https://github.com/katopz/react-chartist-example#readme"
}
